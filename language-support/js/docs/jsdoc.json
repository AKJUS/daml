{
    "plugins": [
        "plugins/commentsOnly",
        "plugins/markdown"
    ],
    "source": {
        "include": [ "./src" ],
        "exclude": [ "./src/tests" ],
        "includePattern": ".+\\.ts$",
        "excludePattern": ".+\\.spec.ts$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": [ "jsdoc" ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "copyright": "Â©2019 Digital Asset Holdings, LLC",
        "outputSourceFiles": false,
        "outputSourcePath": false,
        "includeDate": false
    },
    "opts": {
        "template": "./node_modules/ink-docstrap/template",
        "readme": "./README.md",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }
}
